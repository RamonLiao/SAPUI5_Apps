            <IconTabBar
                id="idIconTabBar"
                headerBackgroundDesign="Transparent"
                class="sapUiSmallMarginBottom sapUiResponsiveContentPadding"
            >
                <items>
                    <IconTabFilter
                        key="home"
                        text="Home"
                    >
                        <mvc:XMLView
                            viewName="ui5.dashboardapp.view.DashboardTabHome"
                        />
                    </IconTabFilter>
                    <IconTabFilter
                        key="assemblyOptions"
                        text="Assembly Options"
                    >
                        <List
                            headerText="A List"
                            showSeparators="Inner"
                        />
                    </IconTabFilter>
                    <IconTabFilter
                        key="contact"
