<mvc:View
    controllerName="ui5.dashboardapp.controller.Dashboard"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
    height="100%"
>
    <Page showHeader="false">
        <content>
            <ObjectHeader
                icon="{images>/avatarImg}"
                iconDensityAware="false"
                iconAlt="Joe Blogs"
                iconActive="true"
                imageShape="Circle"
                responsive="true"
                title="Joe Blogs"
                intro="France"
                class="sapUiResponsivePadding--header"
            />

